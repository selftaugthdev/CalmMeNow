# OpenAI Configuration Template
# Copy this file to .env in your project root and fill in your actual values

# OpenAI API Key (Required)
# Get this from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Organization ID (Optional)
# Only needed if you're part of an OpenAI organization
OPENAI_ORGANIZATION_ID=your_organization_id_here_optional

# API Configuration
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=150
OPENAI_TEMPERATURE=0.7

# App Configuration
APP_ENV=development
DEBUG_MODE=true
